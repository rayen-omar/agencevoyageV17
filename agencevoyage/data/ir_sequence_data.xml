<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_achat" model="ir.sequence">
            <field name="name">ID Achat</field>
            <field name="code">agencevoyage.achat</field>
            <field name="prefix">ACH</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        <record id="sequence_reservation" model="ir.sequence">
            <field name="name">Réservation</field>
            <field name="code">agencevoyage.reservation</field>
            <field name="prefix">RES-%(year)s-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        <record id="sequence_paiement_client" model="ir.sequence">
            <field name="name">Paiement Client</field>
            <field name="code">agencevoyage.paiement.client</field>
            <field name="prefix">PAY-%(year)s-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        <record id="sequence_paiement_fournisseur" model="ir.sequence">
            <field name="name">Paiement Fournisseur</field>
            <field name="code">agencevoyage.paiement.fournisseur</field>
            <field name="prefix">DEC-%(year)s-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        <record id="sequence_caisse" model="ir.sequence">
            <field name="name">Opération Caisse</field>
            <field name="code">agencevoyage.caisse</field>
            <field name="prefix">CAISSE-%(year)s-</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>
